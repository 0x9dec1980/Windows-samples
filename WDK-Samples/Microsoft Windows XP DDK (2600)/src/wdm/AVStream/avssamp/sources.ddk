TARGETNAME=avssamp
TARGETTYPE=DRIVER
TARGETPATH=obj$(BUILD_ALT_DIR)
DRIVERTYPE=WDM

USE_MAPSYM=1

INCLUDES=..\..\..\..\inc;   \
    $(DDK_INC_PATH)\wdm; \
    $(SDK_INC_PATH);

TARGETLIBS=$(DDK_LIB_PATH)\ks.lib

MSC_WARNING_LEVEL=/W3 /WX
C_DEFINES=$(C_DEFINES) -DUNICODE -D_UNICODE -DDEBUG_LEVEL=DEBUGLVL_BLAB

LINKER_FLAGS =$(LINKER_FLAGS) -map -merge:PAGECONST=PAGE

SOURCES=\
        $(TARGETNAME).rc    \
        device.cpp          \
        filter.cpp


MISCFILES=\
        avssamp.inf

