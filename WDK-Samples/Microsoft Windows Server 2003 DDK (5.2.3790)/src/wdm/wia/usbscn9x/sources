!if "$(DDK_TARGET_OS)" == "WinNET"

#
# build ansi always for now
#

NOT_UNICODE=1

TARGETNAME=usbscn9x
TARGETTYPE=DRIVER
TARGETPATH=$(_OBJ_DIR)

TARGETLIBS=$(DDK_LIB_PATH)\usbd.lib

SOURCES= usbscn9x.c \
         ocrw.c  \
         ioctl.c \
         power.c \
         usbscn9x.rc \
         debug.c

